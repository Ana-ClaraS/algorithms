algoritmo "conteate10"
var
   contador, valor, salto: inteiro
inicio
      contador <- 0
      escreva ("quer contar ate quanto?")
      leia (valor)
      escreva ("qual sera o salto? ")
      leia (salto)
      enquanto (contador <= valor) faca
      escreval (contador)
      contador <- contador + salto
      fimEnquanto
      escreva ("terminei de contar")
fimalgoritmo